{
  "name": "@aid-on/memory-rag",
  "version": "1.0.0",
  "description": "Vercel AI SDK対応のインメモリRAG実装 - In-memory RAG implementation with Vercel AI SDK support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.mjs",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md",
    "README.ja.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "test": "NODE_ENV=test vitest run",
    "test:watch": "NODE_ENV=test vitest",
    "test:coverage": "NODE_ENV=test vitest run --coverage",
    "prepublishOnly": "npm run build && npm run type-check",
    "demo:dev": "vite",
    "demo:build": "vite build",
    "demo:preview": "vite preview"
  },
  "keywords": [
    "rag",
    "retrieval-augmented-generation",
    "vector-search",
    "embeddings",
    "ai",
    "llm",
    "memory",
    "vercel-ai-sdk",
    "typescript",
    "openai",
    "anthropic",
    "in-memory"
  ],
  "author": "aid-on",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Aid-On/memory-rag.git"
  },
  "homepage": "https://Aid-On.github.io/memory-rag/",
  "bugs": {
    "url": "https://github.com/Aid-On/memory-rag/issues"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.0",
    "ai": "^5.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^19.1.9",
    "@types/react-dom": "^19.1.7",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vitejs/plugin-react": "^5.0.0",
    "@vitest/coverage-v8": "^2.0.0",
    "eslint": "^8.0.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vite": "^5.0.0",
    "vitest": "^2.0.0"
  },
  "peerDependencies": {
    "@ai-sdk/anthropic": "^2.0.0",
    "@ai-sdk/cohere": "^2.0.0",
    "@ai-sdk/google": "^2.0.0"
  },
  "peerDependenciesMeta": {
    "@ai-sdk/anthropic": {
      "optional": true
    },
    "@ai-sdk/google": {
      "optional": true
    },
    "@ai-sdk/cohere": {
      "optional": true
    }
  }
}
